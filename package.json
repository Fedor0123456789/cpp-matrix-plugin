{
  "name": "third-labwork",
  "displayName": "third labwork",
  "description": "matrix arithmetic",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.2"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "third-labwork.insertMatrixAddition",
        "title": "C++ Matrix: Вставить функцию сложения матриц"
      },
      {
        "command": "third-labwork.insertMatrixSubtraction",
        "title": "C++ Matrix: Вставить функцию вычитания матриц"
      },
      {
        "command": "third-labwork.insertMatrixMultiplication",
        "title": "C++ Matrix: Вставить функцию умножения матриц"
      },
      {
        "command": "third-labwork.insertMatrixInverse",
        "title": "C++ Matrix: Вставить функцию обратной матрицы"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
